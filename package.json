{
  "name": "@4db/cli",
  "version": "1.0.0",
  "description": "Simple NoSQL Database built with SQL PostgreSQL Database",
  "main": "./build/index.js",
  "types": "./build/types/index.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
