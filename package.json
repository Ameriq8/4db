    "prompts": "^2.4.2",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/figlet": "^1.5.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.21",
    "@types/prompts": "^2.4.2",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.13.0",
    "typescript": "^4.6.2"
  }
